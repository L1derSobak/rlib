{
    "-ddoxFilterArgs": [
        "--unittest-examples",
        "--min-protection=Public",
        "--in",
        "rlib",
        "--ex",
        "erupted",
        "--ex",
        "silly"
    ],
    "authors": [
        "Gedert Korney"
    ],
    "configurations": [
        {
            "excludedSourceFiles": [
                "source/app.d"
            ],
            "name": "library",
            "targetType": "library"
        },
        {
            "excludedSourceFiles": [
                "source/app.d"
            ],
            "name": "import",
            "sourceFiles-posix": [
                "librlib.a"
            ],
            "sourceFiles-windows": [
                "rlib.lib"
            ],
            "targetType": "sourceLibrary"
        }
    ],
    
    "copyFiles-windows-x86": ["thirdparty/windows-x86/*.dll"],
    "copyFiles-windows-x86_64": ["thirdparty/windows-x64/*.dll"],
    "copyFiles-linux-x64": ["thirdparty/linux-x64/*.dll"],

    "dependencies": {
        "seh": "~>0.0.22",
        "bindbc-wgpu": "~>0.19.1",
        "bindbc-sdl": "~>1.4.8",
        "semver": "~>0.5.0",
        "singlog": "~>0.5.0",
        "silly": "~>1.1.1"
    },
    "versions": [
        "SDL_2_30"
    ],
    "license": "MIT",
    "name": "rlib"
}
